{
	"name": "autokitteh",
	"private": true,
	"type": "module",
	"version": "1.29.1",
	"scripts": {
		"build": "vite build",
		"build-storybook": "storybook build",
		"build:prod": "NODE_ENV=production npm run build",
		"check-format:staged": "prettier . --check",
		"dev": "npm run validate-env && vite",
		"fix-format:staged": "prettier . --write",
		"lint": "eslint . --ext ts,tsx,json --report-unused-disable-directives --max-warnings=0",
		"lint-fix": "eslint . --ext ts,tsx,json --report-unused-disable-directives --fix",
		"postinstall": "husky install",
		"prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
		"preview": "vite preview",
		"storybook": "storybook dev",
		"tailwind-config-viewer": "tailwind-config-viewer -o --config tailwind.config.cjs",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:report": "npx playwright show-report",
		"test:e2e:ui": "npx playwright test --ui",
		"tsc": "tsc",
		"type-check": "tsc --pretty --noEmit",
		"validate-env": "node ./validateEnv",
		"generate-project-files-index": "node generateProjectFilesIndex.js && eslint --fix src/assets/templates/index.ts"
	},
